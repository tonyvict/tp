@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant ":SearchCommandParser" as SearchCommandParser LOGIC_COLOR
participant "s:SearchCommand" as SearchCommand LOGIC_COLOR
participant ":CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("search alice physics")
activate LogicManager

LogicManager -> AddressBookParser : parseCommand("search alice physics")
activate AddressBookParser

create SearchCommandParser
AddressBookParser -> SearchCommandParser
activate SearchCommandParser
SearchCommandParser --> AddressBookParser
deactivate SearchCommandParser

AddressBookParser -> SearchCommandParser : parse(arguments)
activate SearchCommandParser

create SearchCommand
SearchCommandParser -> SearchCommand : new SearchCommand(predicate)
activate SearchCommand
SearchCommand --> SearchCommandParser : s
deactivate SearchCommand

SearchCommandParser --> AddressBookParser : s
deactivate SearchCommandParser
destroy SearchCommandParser

AddressBookParser --> LogicManager : s
deactivate AddressBookParser

LogicManager -> SearchCommand : execute(model)
activate SearchCommand

SearchCommand -> Model : updateFilteredPersonList(predicate)
activate Model
Model --> SearchCommand
deactivate Model

create CommandResult
SearchCommand -> CommandResult : new CommandResult(summaryMessage)
activate CommandResult
CommandResult --> SearchCommand
deactivate CommandResult

SearchCommand --> LogicManager : r
deactivate SearchCommand

[<-- LogicManager
deactivate LogicManager

@enduml
