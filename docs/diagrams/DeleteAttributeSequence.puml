@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant ":DeleteAttributeCommandParser" as DeleteAttributeCommandParser LOGIC_COLOR
participant "d:DeleteAttributeCommand" as DeleteAttributeCommand LOGIC_COLOR
participant "r:CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "m:Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("delattr 1 attr/subject")
activate LogicManager

LogicManager -> AddressBookParser : parseCommand("delattr 1 attr/subject")
activate AddressBookParser

create DeleteAttributeCommandParser
AddressBookParser -> DeleteAttributeCommandParser
activate DeleteAttributeCommandParser
DeleteAttributeCommandParser --> AddressBookParser
deactivate DeleteAttributeCommandParser

AddressBookParser -> DeleteAttributeCommandParser : parse("1 attr/subject")
activate DeleteAttributeCommandParser

create DeleteAttributeCommand
DeleteAttributeCommandParser -> DeleteAttributeCommand
activate DeleteAttributeCommand
DeleteAttributeCommand --> DeleteAttributeCommandParser
deactivate DeleteAttributeCommand

DeleteAttributeCommandParser --> AddressBookParser : d
deactivate DeleteAttributeCommandParser
DeleteAttributeCommandParser -[hidden]-> AddressBookParser
destroy DeleteAttributeCommandParser

AddressBookParser --> LogicManager : d
deactivate AddressBookParser

' Execute DeleteAttributeCommand
LogicManager -> DeleteAttributeCommand : execute(m)
activate DeleteAttributeCommand

DeleteAttributeCommand -> Model : getFilteredPersonList()
activate Model
Model --> DeleteAttributeCommand : List<Person>
deactivate Model

alt invalid index
  DeleteAttributeCommand --> LogicManager : throw CommandException(MESSAGE_INVALID_PERSON_DISPLAYED_INDEX)
else valid index
  DeleteAttributeCommand -> DeleteAttributeCommand : removeKeys(personToEdit, attributeKeysToDelete)
  DeleteAttributeCommand -> Model : setPerson(personToEdit, editedPerson)
  activate Model
  Model --> DeleteAttributeCommand
  deactivate Model
  DeleteAttributeCommand -> Model : updateFilteredPersonList()
  activate Model
  Model --> DeleteAttributeCommand
  deactivate Model

  create CommandResult
  DeleteAttributeCommand -> CommandResult
  activate CommandResult
  CommandResult --> DeleteAttributeCommand
  deactivate CommandResult

  DeleteAttributeCommand --> LogicManager : r
end
deactivate DeleteAttributeCommand

[<-- LogicManager
deactivate LogicManager
@enduml
